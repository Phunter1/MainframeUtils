DEMOMAC2 BEGIN
         WTO   'DEMOMAC2 COPY DEMOMAC2.DAT TO DEMOMAC2.TST'
         OPEN SYSUT1
         OPEN SYSUT2
LOOP     GET  SYSUT1,RECORD
         PUT  SYSUT2,RECORD
         B    LOOP
EOJ      CLOSE SYSUT1
         CLOSE SYSUT2
         RETURN
RECORD   DS   XL256
SYSUT1   DCB  LRECL=256,RECFM=T,MACRF=G,EODAD=EOJ,
              DDNAME='DEMOMAC2.DAT'
SYSUT2   DCB  LRECL=256,RECFM=T,MACRF=P,
              DDNAME='DEMOMAC2.TST'
         END
